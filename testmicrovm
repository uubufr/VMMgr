qemu-system-x86_64 -pidfile /run/VMMGR/TestMicroVM -name TestMicroVM -uuid 408cd565-959e-4946-ae97-b29feff05097 -machine microvm,accel=kvm,x-option-roms=off,pit=off,pic=off,rtc=off -no-reboot -smp 1,cores=1,threads=1,sockets=1,maxcpus=1 -cpu host,+x2apic,+kvm_pv_eoi,+kvm_pv_unhalt,+kvm_nopiodelay -m 2G -no-user-config -nodefaults -msg timestamp=on -kernel /boot/vmlinuz-5.15.14-100.fc34.x86_64 -append earlyprintk=ttyS0 console=ttyS0 -nographic -serial stdio --mem-prealloc -overcommit cpu-pm=on -overcommit mem-lock=off -device vmgenid     -monitor telnet:swvpl-main:3044,server,nowait
