[remmina]
name=%%NAME%%
password=%%PWD%%
enableaudio=1
resizeguest=1
precommand=
ssh_privatekey=
ssh_enabled=0
ssh_charset=
sharesmartcard=0
ssh_server=
keyboard_grab=0
ssh_username=
cacert=/etc/pki/services/spice/ca.pem
ssh_auth=0
ssh_password=
viewonly=0
ignore-tls-errors=1
postcommand=
server=%%HOSTNAME%%:%%PORT%%
scale=0
ssh_loopback=0
disablepasswordstoring=0
disableclipboard=0
window_maximize=1
group=%%GROUP%%
viewmode=4
window_height=525
window_width=640
protocol=SPICE
usetls=1
